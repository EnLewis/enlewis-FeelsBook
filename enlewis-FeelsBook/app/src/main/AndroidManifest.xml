<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.erikn.enlewis_feelsbook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.erikn.enlewis_feelsbook.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.erikn.enlewis_feelsbook.LogEmotionActivity" />
        <activity
            android:name=".EditEmotionActivity"
            android:label="@string/title_activity_edit_emotion"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.erikn.enlewis_feelsbook.SummaryActivity"></activity>
    </application>

</manifest>